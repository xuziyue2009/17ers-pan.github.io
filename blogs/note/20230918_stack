        栈
        <!-- <pre><code> -->
            #include <bits/stdc++.h>
            using namespace std;
            
            int s[100010], top;
            
            void pop(){
                if (top == 0){
                    cout << "empty" << endl;
                }
                else{
                    top--;
                    cout << s[top] << endl;
                }
            }
            
            void push(int x){
                if (top == n){
                    cout << "full" << endl;
                }
                else{
                    s[top] = x;
                    top++;-
                }
            }
            
            int main(){
                int n, a, b;
                //n:操作次数 a:操作类型(0:push 1:pull) b:压栈数 
                
                cin >> n;
                
                for (int i = 1; i <= n; i++){
                    cin >> a;
                    if (a){
                        pull();
                    }
                    else{
                        cin >> b;
                        push(n);
                    }
                }	
            }
